<div class="page-header">
  <%= link_to cities_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_city_path(@city), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show city</h1>
</div>

<dl class="dl-horizontal">
  <dt>City name:</dt>
  <dd><%= @city.city_name %></dd>

  <dt>State:</dt>
  <dd><%= @city.state %></dd>

  <dt>Us city:</dt>
  <dd><%= @city.us_city %></dd>

  <dt>Population:</dt>
  <dd><%= @city.population %></dd>

  <dt>One hundred pct by:</dt>
  <dd><%= @city.one_hundred_pct_by %></dd>

  <dt>Co2 emissions perannum:</dt>
  <dd><%= @city.co2_emissions_perannum %></dd>

  <dt>Wind:</dt>
  <dd><%= @city.wind %></dd>

  <dt>Solar:</dt>
  <dd><%= @city.solar %></dd>

  <dt>Other energy source:</dt>
  <dd><%= @city.other_energy_source %></dd>

  <dt>Image:</dt>
  <!-- <dd><%# image_tag(@city.image_url) %></dd> -->

</dl>
